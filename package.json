{
  "name": "finten",
  "version": "1.0.0",
  "description": "The hypotenuse of the smallest pythagorean triples.",
  "main": "src/main.ts",
  "scripts": {
    "start": "DB_NAME=secgov-dev ts-node -r dotenv/config src/main.ts",
    "prod": "DB_NAME=secgov ts-node -r dotenv/config src/main.ts",
    "test-script": "NO_DB_INSERT=1 ts-node src/classes/tests/tests.ts",
    "nodemon": "nodemon",
    "test": "mocha -r dotenv/config  -r ts-node/register test/classes/**/*.spec.ts",
    "test-api": "mocha -r dotenv/config -r ts-node/register test/api/**/*.spec.ts",
    "npmi-container": "rm -rf node_modules && npm i"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FaradayDetu/finten.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FaradayDetu/finten/issues"
  },
  "homepage": "https://github.com/FaradayDetu/finten#readme",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.8",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^13.13.5",
    "@types/ora": "^3.2.0",
    "@types/supertest": "^2.0.10",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "chalk": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.26.0",
    "mongodb": "^3.6.2",
    "nodemon": "^2.0.4",
    "ora": "^4.0.4",
    "parse-xbrl": "https://github.com/nikensss/parse-xbrl.git",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^8.1.3"
  }
}