{
  "name": "finten",
  "version": "1.8.2",
  "description": "The hypotenuse of the smallest pythagorean triples.",
  "main": "src/main.ts",
  "scripts": {
    "start": "ts-node -r dotenv/config src/main.ts",
    "temp": "ts-node -r dotenv/config src/temp/temp.ts",
    "nodemon": "nodemon --ignore test",
    "test": "ENV=TEST PORT=3030 DOTENV_CONFIG_PATH=.env.test ts-mocha",
    "test:watch": "nodemon --exec 'npm run test'",
    "full-test": "npm run test -- --forbid-only",
    "learning-test": "npm run test -- 'test/**/*.spec.learning.ts'",
    "test-debug": "npm run test -- --timeout 300000",
    "rimraf": "rm -rf node_modules && npm i",
    "lint": "eslint --fix 'src/**/*.ts' 'test/**/*.spec.ts'",
    "pretty": "prettier --write 'src/**/*.ts' 'test/**/*.spec.ts'",
    "graph": "depcruise --include-only '^src/classes' --output-type dot src | dot -T svg > dependency_graph.svg",
    "stats": "npx sloc src"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      "test"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weirwoodai/finten.git"
  },
  "typings": "types",
  "author": "Alex Faraday, Ricard Mallafre",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/weirwoodai/finten/issues"
  },
  "homepage": "https://github.com/weirwoodai/finten#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty && npm run lint && git add -A",
      "pre-push": "npm run full-test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.27",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^13.13.5",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.7",
    "nodemon": "^2.0.4",
    "ora": "^4.0.4",
    "parse-xbrl": "https://github.com/weirwoodai/parse-xbrl",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.2",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-interface": "^2.0.3",
    "dependency-cruiser": "^9.17.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "mocha": "^8.3.1",
    "mongodb-memory-server": "^6.9.0",
    "prettier": "^2.1.2",
    "ts-mocha": "^8.0.0",
    "ts-mockito": "^2.6.1"
  },
  "engines": {
    "node": "^14.15.1"
  }
}
