{
  "name": "finten",
  "version": "1.0.0",
  "description": "The hypotenuse of the smallest pythagorean triples.",
  "main": "src/main.ts",
  "scripts": {
    "start": "ts-node -r dotenv/config src/main.ts",
    "nodemon": "nodemon",
    "test": "PORT=3030 mocha -r dotenv/config -r ts-node/register test/**/*.spec.ts",
    "full-test": "PORT=3030 mocha --forbid-only -r dotenv/config -r ts-node/register test/**/*.spec.ts",
    "rimraf": "rm -rf node_modules && npm i",
    "lint": "eslint --fix '{src,test}/**/*.ts'",
    "pretty": "prettier --write {src,test}/**/*.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weirwoodai/finten.git"
  },
  "typings": "types",
  "author": "Alex Faraday, Ricard Mallafre",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/weirwoodai/finten/issues"
  },
  "homepage": "https://github.com/weirwoodai/finten#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty && npm run lint && npm run full-test && git add -A",
      "pre-push": "npm run full-test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.5.27",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^13.13.5",
    "@types/ora": "^3.2.0",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "chalk": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.7",
    "nodemon": "^2.0.4",
    "ora": "^4.0.4",
    "parse-xbrl": "https://github.com/weirwoodai/parse-xbrl",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.2",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^7.0.2",
    "@types/mongodb-memory-server": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-interface": "^2.0.3",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "mocha": "^8.1.3",
    "mongodb-memory-server": "^6.9.0",
    "prettier": "^2.1.2",
    "ts-mockito": "^2.6.1"
  },
  "engines": {
    "node": ">=12.19.0"
  }
}
