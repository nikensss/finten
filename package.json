{
  "name": "finten",
  "version": "1.0.0",
  "description": "The hypotenuse of the smallest pythagorean triples.",
  "main": "src/main.ts",
  "scripts": {
    "start": " ts-node -r dotenv/config src/main.ts",
    "prod": "ts-node -r dotenv/config src/main.ts",
    "test-script": "ts-node src/classes/tests/tests.ts",
    "nodemon": "nodemon",
    "test": "mocha -r dotenv/config  -r ts-node/register test/classes/**/*.spec.ts",
    "test-api": "mocha -r dotenv/config -r ts-node/register test/api/**/*.spec.ts",
    "npmi-container": "rm -rf node_modules && npm i",
    "pre-deploy": "rm -rf node_modules && cd db_docker_test && docker-compose up --exit-code-from test"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weirwood/finten.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/weirwood/finten/issues"
  },
  "homepage": "https://github.com/weirwood/finten#readme",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.5.27",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^13.13.5",
    "@types/ora": "^3.2.0",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "chalk": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.7",
    "nodemon": "^2.0.4",
    "ora": "^4.0.4",
    "parse-xbrl": "https://github.com/weirwood/parse-xbrl.git",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.2",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^7.0.2",
    "@types/mongodb-memory-server": "^2.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^8.1.3",
    "mongodb-memory-server": "^6.9.0"
  }
}