{
  "name": "finten",
  "version": "1.0.0",
  "description": "The hypotenuse of the smallest pythagorean triples.",
  "main": "src/main.ts",
  "scripts": {
    "start": "ts-node src/main.ts",
    "nodemon": "nodemon",
    "test": "mocha --require ts-node/register test/**/*.spec.ts",
    "test-custom": "ts-node src/tests/tests.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FaradayDetu/finten.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FaradayDetu/finten/issues"
  },
  "homepage": "https://github.com/FaradayDetu/finten#readme",
  "dependencies": {
    "@feathersjs/errors": "^4.5.3",
    "@feathersjs/express": "^4.5.4",
    "@feathersjs/feathers": "^4.5.3",
    "@feathersjs/socketio": "^4.5.4",
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.6",
    "@types/moment": "^2.13.0",
    "@types/nedb": "^1.8.9",
    "@types/node": "^13.13.5",
    "@types/ora": "^3.2.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "feathers-nedb": "^5.1.0",
    "moment": "^2.26.0",
    "nedb": "^1.8.0",
    "nodemon": "^2.0.4",
    "ora": "^4.0.4",
    "parse-xbrl": "https://github.com/nikensss/parse-xbrl.git",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^8.0.1"
  }
